<Window x:Class="TeamCord.GUI.ChannelConnector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeamCord.GUI"
        mc:Ignorable="d"
        Title="ChannelConnector" Height="350" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="40" />
        </Grid.RowDefinitions>
        <TreeView Name="treeView_items" Grid.Row="0">
        </TreeView>
        <Button Name="button_Apply" Content="Apply" Grid.Row="1" Click="button_Apply_Click" />
    </Grid>
</Window>
<Window x:Class="TeamCord.GUI.VolumeControl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeamCord.GUI"
        mc:Ignorable="d"
        WindowStyle="ToolWindow"
        ResizeMode="NoResize"
        WindowStartupLocation="Manual"
        Title="Volume" Height="250" Width="300">
    <Grid>
        <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Disabled" HorizontalAlignment="Center">
            <StackPanel x:Name="stackPanelSliders" Orientation="Horizontal" HorizontalAlignment="Center">
                <TextBlock Name="textBlock_noUsers" Text="No other users in discord channel" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Collapsed" />
            </StackPanel>
        </ScrollViewer>
    </Grid>
</Window>